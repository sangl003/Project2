<div id="home">
  <div class="container">
    <div class="row">
      <div class="col-8 offset-2">
        <h1><center>Add your favorite API</center></h1>
        <hr>
        <form class="clearfix mb-4" action="POST">
          <div class="form-group">
            <label for="api-text">API NAME</label>
            <textarea class="form-control" id="api-text" aria-describedby="api-text" placeholder="Enter API Name"></textarea>
          </div>
          <div class="form-group">
            <label for="api-description">API DESCRIPTION</label>
            <textarea class="form-control" id="api-description" aria-describedby="api-description" placeholder="Enter API Description"></textarea>
          </div>
          <div class="form-group">
            <label for="api-category">API CATEGORY</label>
            <textarea class="form-control" id="api-category" aria-describedby="api-category" placeholder="Enter API Category"></textarea>
          </div>
        <div class="form-group">
            <label for="api-URL">API URL</label>
            <textarea class="form-control" id="api-URL" aria-describedby="api-URL" placeholder="Enter API URL"></textarea>
        </div>
        <div class="form-group">
            <label for="api-Authentication">API AUTHENTICATION</label>
            <textarea class="form-control" id="api-Authentication" aria-describedby="api-Authentication" placeholder="Enter API Authentication"></textarea>
        </div>
          <button id="submit" class="btn btn-primary float-right">Submit</button>
        </form>
        <h3><center>API List:</center></h3>
        <hr>
        <ul id="api-list" class="list-group">
          {{#each apis}}
          <li data-id="{{this.id}}" class="list-group-item">
            <a href="apis/{{this.id}}">{{this.text}}</a>
            {{!-- <button class="btn btn-danger float-right delete">ï½˜</button> --}}
          </li>
          {{/each}}
        </ul>
      </div>
    </div>
  </div>
</div>
